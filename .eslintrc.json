{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "import/extensions": "off",
      "import/prefer-default-export": "off",
      "class-methods-use-this": "off",
      "no-underscore-dangle": "off"
    },
    "settings": {
      "import/resolver": {
        "typescript": {}
      }
    }
}
